// https://developer.chrome.com/extensions/manifest
{
  "manifest_version": 2,

  "name": "ChromeExtensionTest",
  "version": "0.0.1",

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["jquery-1.11.3.min.js", "helloWorld.js"]
    }
  ],

  "browser_action": {
    "default_popup": "browserAction.html"
  }
}
